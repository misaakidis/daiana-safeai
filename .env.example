# Required Environment Variables (must be set)
#----------------------------------------
DISCORD_APPLICATION_ID=           # Your Discord application ID
DISCORD_API_TOKEN=               # Your Discord bot token
VENICE_API_KEY=                  # Your Venice API key

# Venice Model Configuration
#----------------------------------------
SMALL_VENICE_MODEL=deepseek-r1-llama-70b
MEDIUM_VENICE_MODEL=deepseek-r1-llama-70b
LARGE_VENICE_MODEL=deepseek-r1-671b
IMAGE_VENICE_MODEL=flux-dev

# ElevenLabs Configuration
#----------------------------------------
ELEVENLABS_XI_API_KEY=          # API key for voice synthesis
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_VOICE_ID=21m00Tcm4TlvDq8ikWAM
ELEVENLABS_VOICE_STABILITY=0.5
ELEVENLABS_VOICE_SIMILARITY_BOOST=0.9
ELEVENLABS_VOICE_STYLE=0.66
ELEVENLABS_VOICE_USE_SPEAKER_BOOST=false
ELEVENLABS_OPTIMIZE_STREAMING_LATENCY=4
ELEVENLABS_OUTPUT_FORMAT=pcm_16000

# Twitter Integration
#----------------------------------------
TWITTER_DRY_RUN=false           # Set to true for testing without posting
TWITTER_USERNAME=               # Twitter account username
TWITTER_PASSWORD=               # Twitter account password
TWITTER_EMAIL=                  # Twitter account email
TWITTER_COOKIES=                # Twitter session cookies

# XAI Configuration
#----------------------------------------
X_SERVER_URL=                   # XAI server endpoint
XAI_API_KEY=                    # XAI authentication key
XAI_MODEL=                      # XAI model identifier

# Posting Configuration
#----------------------------------------
POST_INTERVAL_MIN=90            # Minimum time between posts (minutes)
POST_INTERVAL_MAX=180           # Maximum time between posts (minutes)

# Wallet Configuration
#----------------------------------------
WALLET_PRIVATE_KEY=             # Your wallet private key
WALLET_PUBLIC_KEY=              # Your wallet public key

# Additional API Keys
#----------------------------------------
BIRDEYE_API_KEY=               # Birdeye API authentication
TELEGRAM_BOT_TOKEN=            # Telegram bot authentication
TOGETHER_API_KEY=              # Together AI platform key

# Server Configuration
#----------------------------------------
SERVER_PORT=3000               # Main application port
IMAGE_GEN=false               # Enable/disable image generation
DAEMON_PROCESS=false          # Enable/disable daemon mode
